@foreach (var item in Model)
{
    <div class="card d-flex text-center" style="width:18rem;">
            <div class="card-title">
                <h4>Id: @item.Id</h4>
            </div>
            <div class="card-body">
                <p>Имя: @item.Name</p>
                <p>Фамилия: @item.Surname</p>
                <p>Почта: @item.EMail</p>
                <p>Телефон: @item.Phone</p>
                <p>Пароль: @item.Password</p>
                <p>Активность: @item.IsActive</p>
            </div>
        </div>
}

